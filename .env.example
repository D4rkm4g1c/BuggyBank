# Backend Configuration
NODE_ENV=development
PORT=3001

# Database
DB_PATH=./buggybank.db

# JWT Configuration (Weak by design)
JWT_SECRET=buggybank-secret-key

# File Upload
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# WebSocket
WS_PORT=3001

# Security (Disabled for vulnerabilities)
ENABLE_RATE_LIMITING=false
ENABLE_CSRF_PROTECTION=false
ENABLE_HELMET=false 